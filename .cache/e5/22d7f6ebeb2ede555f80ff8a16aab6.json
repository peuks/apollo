: 2.2rem;\n  --fs-h2: 2rem;\n  --fs-h3: 1.6rem;\n  --fs-h4: 1.4rem;\n  --fs-h5: 1.2rem;\n  --fs-body: 1rem;\n\n  // Espacements\n\n  // Shadow\n\n  // Buttons\n}\n\n/* Reset class*/\n* {\n  box-sizing: inherit;\n  padding: 0;\n  margin: 0;\n}\n\nhtml {\n  box-sizing: border-box;\n  // font-size: clamp(100%, calc(1rem + 2vw), 24px);\n  // font-size: clamp(100%, calc(1rem + 2vw), 24px);\n}\n\nhtml,\nbody {\n  // Va dépendre du projet\n  width: 100%;\n  height: auto;\n  background-color: var(--clr-1);\n  color: #f7f7f9;\n}\n\nbody {\n  line-height: 1.5;\n  font-family: var(--ff);\n}\n\n/**\n* Preventing Text Overflow \n*/\np,\nli,\nh1,\nh2,\nh3,\nh4 {\n  // Help prevent overflow of long words/names/URLs\n  overflow-wrap: break-word;\n\n  // Optional, not supported for all languages\n  hyphens: auto;\n}\n\n$type-ratios: (\n  \"minorSecond\": 1.067,\n  \"majorSecond\": 1.125,\n  \"minorThird\": 1.2,\n  \"majorThird\": 1.25,\n  \"perfectFourth\": 1.333,\n  \"augmentedFourth\": 1.414,\n  \"perfectFifth\": 1.5,\n  \"goldenRatio\": 1.618,\n);\n\n// Recommended starting point\n$type-base-size: 1rem;\n\n// Select by key of map, or use a custom value\n$type-size-ratio: type-ratio(minorSecond);\n\n// List in descending order to prevent extra sort function\n$type-levels: 4, 3, 2, 1;\n\n$level-size: $type-base-size;\n\n@each $level in $type-levels {\n  $level-size: $level-size * map-get($type-ratios, \"perfectFifth\");\n\n  h#{$level} {\n    font-size: $level-size;\n  }\n}\n@media screen and (min-width: 560px) {\n}\n\n@media screen and (min-width: 700px) {\n}\n\n@media screen and (min-width: 960px) {\n}\n\n@media screen and (min-width: 1400px) {\n}\nimg {\n}\n\na {\n  color: #f7f7f9;\n  text-decoration: none;\n}\nul {\n  list-style-type: none;\n}\nmain {\n  // height: 100%;\n  // display: flex;\n  // justify-content: center;\n  // flex-direction: column;\n  // align-items: center;\n}\n\n.container {\n  // display: flex;\n  // justify-content: center;\n  // flex-direction: column;\n  // align-items: center;\n\n  // max-width: 50em;\n  // border: solid 1px white;\n}\n\n.wrapp {\n  --pading: 4rem clamp(0.8rem, 2vw + 1rem, 3rem);\n  padding: var(--pading, 4rem);\n  margin-right: auto;\n  margin-left: auto;\n}\n.spacing > * + * {\n  margin-top: var(--spacer, 2rem);\n}\n\n.center {\n  display: flex;\n  justify-content: center;\n  flex-direction: column;\n  align-items: center;\n}\n\n.split {\n  --gap: 2em;\n  display: flex;\n\n  gap: var(--gap);\n  flex-wrap: wrap;\n  justify-content: center;\n\n  & > * {\n    //\n    --min-width: 15em;\n    --flex-basis: 30%;\n    flex-basis: var(--flex-basis, 30%);\n    min-width: var(--min-width, 15em);\n    animation: myanim 4s;\n    flex: 1;\n    //\n    @keyframes myanim {\n      50% {\n        flex: 1;\n      }\n    }\n  }\n}\n\nh2 {\n  span {\n    display: block;\n  }\n}\n.section__title {\n  text-align: center;\n}\n// Navbar\n.burger {\n  img {\n    height: 20px;\n    width: 40px;\n    filter: sepia(1);\n  }\n}\n\n.navbar {\n  background-color: #3f51b5;\n  width: 100%;\n  position: sticky;\n  top: 0;\n  .wrapp {\n    --pading: 1rem 2rem;\n  }\n  .container {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    .logo {\n      width: 128px;\n      height: auto;\n    }\n    .items {\n      display: none;\n      @media screen and (min-width: 960px) {\n        display: flex;\n        gap: 2rem;\n      }\n    }\n    .burger {\n      @media screen and (min-width: 960px) {\n        display: none;\n      }\n    }\n  }\n}\n\n// section hero\n\n.hero {\n  color: var(--clr-4);\n  .container {\n    min-height: calc(100vh - 12rem);\n  }\n\n  // text-align: center;\n\n  // padding: 1em 0;\n\n  background-color: #111; //*Incase background-image brakes\n  &__title,\n  &__copy {\n    @media screen and (min-width: 960px) {\n      text-align: center;\n    }\n  }\n  &__title {\n    font-size: 2.488rem; //*Incase clamp isn't support\n    font-size: clamp(2.488rem, 5vw + 1rem, 4.209rem);\n    line-height: 1.5; //* Lower line height is possible if uppercase\n    text-transform: uppercase;\n  }\n\n  @supports (background-blend-mode: multiply) {\n    background: url(\"../assets/img/Hero.png\"), radial-gradient(#444, #111);\n    background-blend-mode: multiply; // *Melt the image with gradient\n    background-attachment: fixed; // *Parallax effect\n    background-repeat: no-repeat;\n    background-position: center center;\n    background-size: cover;\n  }\n}\n\n.card {\n  box-shadow: 0 1px 8px 0 rgb(73 86 92 / 10%), 0 4px 6px 0 rgb(73 86 92 / 12%);\n  line-height: 1.38;\n  flex-direction: column;\n  background-color: #fff;\n  margin-top: 1.1875rem;\n  box-shadow: 0 1px 8px 0 rgb(73 86 92 / 10%), 0 4px 6px 0 rgb(73 86 92 / 12%);\n  border-radius: 0.625rem;\n  color: var(--clr-1);\n}\n\n.services {\n  .section__title {\n    font-size: clamp(1.5rem, 1vw + 1rem, 2.5rem);\n  }\n}\n.search {\n  height: 2.5rem;\n  display: flex;\n  width: clamp(15em, 20em + 18vw, 50%);\n  & > * {\n    border: none;\n  }\n  input {\n    flex: 10;\n    border-radius: 1.5625rem 0 0 1.5625rem;\n  }\n  input::placeholder {\n    padding-left: 1.8rem;\n  }\n  button {\n    border-radius: 0 1.5625rem 1.5625rem 0;\n    background: var(--clr-3);\n    flex: 3;\n    padding: 0 1em;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"97c89efbf542665b23ee229019ffff02","cacheData":{"env":{}}}